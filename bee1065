#include <stdio.h>

int main() {
    int value, even_count = 0;

    // Loop to read 5 integer values
    for (int i = 0; i < 5; i++) {
        scanf("%d", &value);
        if (value % 2 == 0) {
            even_count++;
        }
    }

    // Print the number of even values
    printf("%d valores pares\n", even_count);

    return 0;
}
