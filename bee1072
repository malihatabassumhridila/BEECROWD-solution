#include <stdio.h>

int main() {
    int n, x;
    int in = 0, out = 0;

    // Read the number of test cases
    scanf("%d", &n);

    // Iterate through the numbers
    for (int i = 0; i < n; i++) {
        scanf("%d", &x);

        // Check if the number is in the interval [10, 20]
        if (x >= 10 && x <= 20) {
            in++;
        } else {
            out++;
        }
    }

    // Output the results
    printf("%d in\n", in);
    printf("%d out\n", out);

    return 0;
}
